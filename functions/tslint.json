{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {
      "object-literal-sort-keys": false
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "semicolon": [true, "never"],
    "quotemark": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-console": false
  },
  "rulesDirectory": []
}