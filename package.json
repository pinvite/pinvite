    "gatsby-image": "^2.0.26",
    "gatsby-plugin-create-client-paths": "^2.0.2",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.2",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-sharp": "^2.1.10",
    "history": "^4.7.2",
    "polished": "^2.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "save": "^2.3.2",
    "styled-components": "^4.0.2",
    "whatwg-fetch": "^2.0.4"
  },
  "keywords": [],
  "scripts": {
    "copyFilesToFunctions": "copyfiles -f public/users/*.html functions/src/users && copyfiles -f src/domain/*.ts functions/src/domain && copyfiles -f src/protocols/*.ts functions/src/protocols",
    "buildFunctions": "cd functions && npm run build",
    "buildHosting": "gatsby build && npm run copyFilesToFunctions",
    "build": "npm run format && npm run lint && npm run tsc && npm run buildHosting && npm run buildFunctions",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.ts src/**/*.tsx",
    "test": "cypress run",
    "lint": "tslint --fix src/**/*.ts src/**/*.tsx -p tsconfig.json",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@types/core-js": "^2.5.0",
    "@types/node": "^10.12.11",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.2",
