    "cypress": "^3.1.4",
    "firebase-tools": "^6.2.2",
    "prettier": "^1.14.2",
    "prettierrc": "0.0.0-5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
